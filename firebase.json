{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,

    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "serving",
          "region": "us-central1"
        }
      }
    ]
  },

  "functions": {
    "source": "functions"
  }
}
